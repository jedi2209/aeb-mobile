{
  "name": "Aebrus",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "postinstall": "cd ./ios; pod update; pod install; cd ../; npx jetify;",
    "test": "jest",
    "lint": "eslint .",
    "clean": "react-native-clean-project",
    "iphone": "react-native run-ios --simulator=\"iPhone 11\"",
    "iphonePro": "react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "iphoneProMax": "react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "iphone-release": "fastlane ios beta;",
    "react": "react-native start --reset-cache",
    "android": "cd ./android && ./gradlew clean; cd ../; open -a iTerm ./android/dev.sh & adb wait-for-device; sleep 10; react-native run-android",
    "android-release": "cd ./android && ./gradlew clean; bundle exec fastlane android internal; cd ../",
    "android-build": "cd ./android && ./gradlew clean; bundle exec fastlane android build; cd ../; open ./android/app/build/outputs/apk/release;",
    "version": "./version.sh"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "appcenter": "2.5.0",
    "appcenter-analytics": "2.5.0",
    "appcenter-crashes": "2.5.0",
    "effector": "^20.10.0",
    "effector-react": "^20.6.0",
    "i18n-js": "^3.5.1",
    "lodash": "^4.17.15",
    "lodash.memoize": "^4.1.2",
    "moment": "^2.24.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-add-calendar-event": "^3.0.0",
    "react-native-autoheight-webview": "^1.4.0",
    "react-native-calendar-events": "^1.7.3",
    "react-native-calendars": "^1.221.0",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-localize": "^1.3.2",
    "react-native-map-link": "^2.7.0",
    "react-native-maps": "0.26.1",
    "react-native-onesignal": "^3.6.2",
    "react-native-permissions": "^2.0.9",
    "react-native-picker-select": "^6.3.2",
    "react-native-reanimated": "^1.4.0",
    "react-native-render-html": "^4.2.0",
    "react-native-segmented-control-tab": "^3.4.1",
    "react-native-snap-carousel": "^3.8.4",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^9.13.6",
    "react-native-tab-view": "^2.11.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^7.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.3",
    "reanimated-bottom-sheet": "^1.0.0-alpha.18"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@react-native-community/eslint-config": "0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "jetifier": "^1.6.5",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-native-clean-project": "^3.3.0",
    "react-native-svg-transformer": "^0.13.0",
    "react-test-renderer": "16.9.0",
    "reactotron-react-native": "^4.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
