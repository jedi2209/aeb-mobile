{
  "name": "Aebrus",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "postinstall": "cd ./ios; pod update; pod install; cd ../;",
    "test": "jest",
    "lint": "eslint .",
    "iphone": "react-native run-ios --simulator=\"iPhone 11\"",
    "iphonePro": "react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "iphoneProMax": "react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "iphone-release": "fastlane ios beta;",
    "react": "react-native start --reset-cache",
    "android": "open -a Terminal ./android/dev.sh & adb wait-for-device; sleep 10; react-native run-android",
    "android-release": "cd ./android; bundle exec fastlane android internal; cd ../",
    "android-build": "cd ./android; bundle exec fastlane android build; cd ../; open ./android/app/build/outputs/apk/release;",
    "version": "./version.sh"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.2",
    "appcenter": "2.5.0",
    "appcenter-analytics": "2.5.0",
    "appcenter-crashes": "2.5.0",
    "effector": "^20.4.4",
    "effector-react": "^20.4.0",
    "i18n-js": "^3.3.0",
    "lodash": "^4.17.15",
    "lodash.memoize": "^4.1.2",
    "moment": "^2.24.0",
    "react": "16.8.6",
    "react-native": "0.60.0",
    "react-native-calendars": "^1.214.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-localize": "^1.2.1",
    "react-native-onesignal": "^3.5.0",
    "react-native-picker-select": "^6.3.2",
    "react-native-reanimated": "^1.2.0",
    "react-native-render-html": "^4.1.2",
    "react-native-segmented-control-tab": "^3.4.1",
    "react-native-svg": "^9.6.4",
    "react-native-webview": "^7.5.1",
    "react-navigation": "^3.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/runtime": "^7.5.4",
    "@react-native-community/eslint-config": "0.0.5",
    "babel-jest": "^24.8.0",
    "eslint": "^6.0.1",
    "jest": "^24.8.0",
    "jetifier": "^1.6.4",
    "metro-react-native-babel-preset": "^0.55.0",
    "react-native-svg-transformer": "^0.13.0",
    "react-test-renderer": "16.8.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
