{
  "name": "Aebrus",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "android": "cd ./android && ./gradlew clean && cd ../; open -a iTerm ./android/dev.sh & adb wait-for-device; sleep 10; react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build": "yarn run android-release; yarn run iphone-release;",
    "postinstall": "npx pod-install; npx jetify;",
    "clean": "npx react-native-clean-project",
    "iphone": "react-native run-ios --simulator=\"iPhone 11\"",
    "iphonePro": "react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "iphoneProMax": "react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "iphone-release": "bundle exec fastlane ios beta; rm Aebrus.app.dSYM.zip;",
    "react": "react-native start --reset-cache",
    "android-release": "cd ./android && ./gradlew clean && cd ../; bundle exec fastlane android internal;",
    "android-build": "cd ./android && ./gradlew clean && cd ../; bundle exec fastlane android release && open ./android/app/build/outputs/apk/release;"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.9",
    "@react-native-picker/picker": "^2.2.0",
    "@sentry/react-native": "^3.1.1",
    "effector": "^22.1.2",
    "effector-react": "^22.0.4",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.21",
    "lodash.memoize": "^4.1.2",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-add-calendar-event": "^4.0.0",
    "react-native-autoheight-webview": "^1.5.8",
    "react-native-calendar-events": "^2.2.0",
    "react-native-calendars": "^1.1267.0",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-localize": "^2.1.5",
    "react-native-map-link": "^2.8.0",
    "react-native-maps": "^0.29.3",
    "react-native-onesignal": "^4.3.2",
    "react-native-permissions": "^3.1.0",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "^2.2.3",
    "react-native-render-html": "^6.1.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-segmented-control-tab": "^4.0.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-vector-icons": "^9.0.0",
    "react-navigation": "^4.4.4",
    "react-navigation-stack": "^2.10.4",
    "reanimated-bottom-sheet": "^1.0.0-alpha.22"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/runtime": "^7.15.4",
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-jest": "^27.2.5",
    "eslint": "^8.0.1",
    "jest": "^27.2.5",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  },
  "resolutions": {
    "@react-native-picker/picker": "^2.2.0"
  }
}
